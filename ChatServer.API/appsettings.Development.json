{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Server=$SQL_HOST;Port=$SQL_PORT;Database=$SQL_DB;User Id=$SQL_USER;Password=$SQL_PASSWORD"
    },
    "Jwt": {
        "Issuer": "https://localhost:7172",
        "JWKS": "https://localhost:7172/.well-known/jwks.json"
    }
}